<form>
<table cellpadding="0" cellspacing="0" border="0" class="table table-bordered table-condensed table-striped table-hover" id="commercialDocumentTable">
   <thead>
       <tr>
           <th>{% trans %}Sel{% endtrans %}</th>
           <th>{% trans %}Title{% endtrans %}</th>
           <th>{% trans %}Date{% endtrans %}</th>
       </tr>
   </thead>
   <tbody>
      {% for row in availableInterventions %}
           <tr>
               <td><input type="checkbox" name="selected[]" value="{{ row.id }}"></td>
               <td>{{ row.title }}</td>
               <td>{{ row.date|date("d/m/Y") }}</td>                          
           </tr>
     {% endfor %}
   </tbody>
</table>
</form>